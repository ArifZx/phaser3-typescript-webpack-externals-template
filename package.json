{
  "name": "phaser3-typescript-webpack-externals-template",
  "version": "1.0.0",
  "description": "A Phaser 3 project template with ES6 support via [Webpack 4](https://webpack.js.org/) and [Babel 7](https://babeljs.io/) that includes hot-reloading for development and production-ready builds with externals `phaser.min.js`.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "start": "node index.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js --hot",
    "dev-host": "webpack-dev-server --config webpack.dev.config.js --hot --host 0.0.0.0 --disable-host-check",
    "dev-stat": "webpack --config webpack.dev.config.js --json > stats.json",
    "stat": "webpack --mode production --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArifZx/phaser3-typescript-webpack-externals-template.git"
  },
  "author": "Arif Budiman <arif.budiman.10.96@gmail.com> (http://www.arifz.com)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "homepage": "https://github.com/ArifZx/phaser3-typescript-webpack-externals-template#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.4",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "phaser": "^3.22.0",
    "prettier": "^2.0.4",
    "raw-loader": "^4.0.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "uglify-js": "^3.9.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "express": "^4.17.3"
  },
  "bugs": {
    "url": "https://github.com/ArifZx/phaser3-typescript-webpack-externals-template/issues"
  },
